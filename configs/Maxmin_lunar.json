{
  "env": [
    {
      "name": ["LunarLander-v2"],
      "max_episode_steps": [1000],
      "input_type": ["feature"]
    }
  ],
  "agent": [
    {
      "name": ["DQN", "DDQN"]
    },
    {
      "name": ["MaxminDQN", "AveragedDQN"],
      "target_networks_num": [2,3,4,5,6,7,8,9]
    }
  ],
  "train_steps": [1.5e6],
  "test_per_episodes": [-1],
  "device": ["cpu"],
  "hidden_layers": [[64,64]],
  "memory_type": ["FiniteReplay"],
  "memory_size": [1e4],
  "exploration_type": ["LinearEpsilonGreedy"],
  "exploration_steps": [1e3],
  "epsilon_steps": [1e3],
  "epsilon_start": [1.0],
  "epsilon_end": [0.01],
  "epsilon_decay": [0.999],
  "loss": ["MSELoss"],
  "optimizer": [
    {
      "name": ["RMSprop"],
      "kwargs": [{"lr": [3e-3, 1e-3, 3e-4, 1e-4, 3e-5]}]
    }
  ],
  "batch_size": [32],
  "display_interval": [100],
  "rolling_score_window": [{"Train": [100], "Test": [10]}],
  "discount": [0.99],
  "seed": [1],
  "show_tb": [false],
  "gradient_clip": [5],
  "target_network_update_steps": [200],
  "network_update_steps": [1],
  "generate_random_seed": [true]
}
